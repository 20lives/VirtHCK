sel disk 1
create partition extended
create partition logical size=2048
assign letter=g
format fs=ntfs label=NTFS quick
create partition logical size=2048
assign letter=i
format fs=ntfs label=CNTFS compress quick
create partition logical size=1024
assign letter=k
format fs=fat label=FAT quick
create partition logical size=1024
assign letter=l
format fs=fat32 label=FAT32 quick
create partition logical size=2048
assign letter=m
format fs=exfat label=ExFAT quick
create partition logical size=2048
assign letter=n
format fs=udf label=UDF quick
create partition logical size=10240
assign letter=o
format fs=refs label=REFS quick